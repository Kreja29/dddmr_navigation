<launch>

  <node pkg="lego_loam_bor" exec="lego_loam" output="screen" respawn="false">
    <param from="$(find-pkg-share lego_loam_bor)/config/loam_os1_config.yaml" />
    <remap from="/lslidar_point_cloud" to="/ouster/points"/>
    <remap from="/odom" to="/lynx/odometry/filtered"/>
  </node>

  <node pkg="global_planner" exec="global_planner_node" output="screen" respawn="false">
    <param from="$(find-pkg-share p2p_move_base)/config/p2p_move_base_mapping_os1.yaml" />
  </node>

  <node pkg="p2p_move_base" exec="p2p_move_base_node" output="screen" respawn="false">
    <param from="$(find-pkg-share p2p_move_base)/config/p2p_move_base_mapping_os1.yaml" />
    <remap from="cmd_vel_stamped" to="/lynx/cmd_vel"/>
  </node>
  <node pkg="p2p_move_base" exec="clicked2goal.py" output="screen" respawn="false"/>

</launch>
